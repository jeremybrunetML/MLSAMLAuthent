    "server-name": "%%samlAppServerName%%",
    "server-type": "http",
    "root": "/",
    "group-name": "%%GROUP%%",
    "port": "%%samlAppServerPort%%",
    "modules-database": "%%mlModulesDbName%%",
    "content-database": "%%mlFinalDbName%%",
